@{
    ViewBag.Title = "Cash Flow ";
}

<script src="~/Scripts/jquery.cycle.lite.js"></script>

<script type="text/javascript">
    $().ready(function()  {

        $('#bg_containers').cycle({
            speed: 3000
        });

    });
                 
</script>
 
<style type="text/css">
 
/* classes to work with cycle plugin for BG images */
#bg_containers {width:100%; margin:0;}
#bg_containers div {width:100%; height:800px; margin:0 auto;}
#bg_containers .bg1 {background: url(../../CashFlowManager/Content/Images/CSR-in-Business.jpg) no-repeat;background-size:100%}
#bg_containers .bg2 {background:url(../../CashFlowManager/Content/Images/website-photo-state-incentive-page.jpg) no-repeat;background-size:100%}
#bg_containers .bg3 {background:url(../../CashFlowManager/Content/Images/Questions.jpg) no-repeat;background-size:100%}
</style>
 
 <section>
 <div id="bg_containers">
     <div class="bg1"></div>
     <div class="bg2"></div>
     <div class="bg3"></div>
 </div>
</section>
